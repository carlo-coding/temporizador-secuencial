{
  "expo": {
    "name": "Temporizador Secuencial",
    "slug": "temporizador-secuencial",
    "scheme": "tempsec",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "android": {
      "package": "com.tu.nombre.temporizadorsecuencial",
      "permissions": ["VIBRATE"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        { "icon": "./assets/notification-icon.png", "color": "#000000" }
      ]
    ],
    "extra": {
      "eas": { "projectId": "FILL_ME_BY_EAS" }
    }
  }
}
